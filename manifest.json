{
    "manifest_version": 2,
   
    "name": "Find Words and Alert",
    "version": "1.1.1",
    "description": "Finds specified words/regex on a webpage and alerts you",
    "options_page": "options.html",
    "permissions": [
        "activeTab",
        "storage"
      ],
    "content_scripts": [{
      "js": ["index.js"],
      "matches": ["*://*/*"]
    }]
   
  }